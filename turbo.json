{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "env": [
        "ELEVENLABS_API_KEY"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "//#lint": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": true
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}